{
	"brackets": "never",
	"colons": "never",
	"commaSpace": "always",
	"commentSpace": "always",
	"duplicates": true,
	"efficient": "always",
	"exclude": ["node_modules/**/*", "bower_components/**/*"],
	"extendPref": "@extend",
	"globalDupe": false,
	"leadingZero": "true",
	"mixed": true,
	"namingConvention": "camelCase",
	"none": "true",
	"noImportant": true,
	"placeholder": "always",
	"prefixVarsWithDollar": "always",
	"quotePref": "single",
	"semicolons": "never",
	"stackedProperties": "never",
	"trailingWhitespace": "never",
	"universal": false,
	"valid": "true",
	"zeroUnits": "never"
}
